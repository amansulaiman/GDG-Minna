<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gdgminna.blogspot.com.GDGBells_University">
    <ProgressBar
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/progressBar7"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/webView7"
        android:layout_alignStart="@+id/webView7"
        android:layout_alignRight="@+id/webView7"
        android:layout_alignEnd="@+id/webView7" />

    <WebView
        android:layout_width="wrap_content"
        android:layout_height="479dp"
        android:id="@+id/webView7"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/progressBar7" />


</RelativeLayout>
