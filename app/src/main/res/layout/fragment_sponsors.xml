<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gdgminna.blogspot.com.Sponsors">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@drawable/banner"
        android:gravity="center"
        android:id="@+id/relativeLayout2"
        android:layout_gravity="left|top" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Current Sponsors"
        android:textStyle="bold"
        android:textColor="#ff00aaff"
        android:textSize="25dp"
        android:id="@+id/textView9"
        android:layout_marginTop="30dp"
        android:layout_gravity="center_horizontal"
        android:layout_below="@+id/relativeLayout2"
        android:layout_centerHorizontal="true" />


    <View
        android:layout_width="match_parent"
        android:layout_height="3px"
        android:background="#ff067aff"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="10dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <View
        android:layout_width="match_parent"
        android:layout_height="3px"
        android:layout_marginTop="20dp"
        android:background="#ffff0014"
        android:layout_below="@+id/imagegoogle"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

     <ImageView
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/imagegoogle"
         android:layout_marginLeft="12dp"
        android:background="@drawable/google"
        android:layout_marginTop="90dp"
        android:layout_below="@+id/relativeLayout2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <ImageView
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:id="@+id/imagedevelopers"
        android:background="@drawable/develop"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="12dp"
        android:layout_below="@+id/imagegoogle"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/imagegoogle"
        android:layout_alignEnd="@+id/imagegoogle" />
    <View
        android:layout_width="match_parent"
        android:layout_height="3px"
        android:background="#ffffe468"
        android:layout_below="@+id/imagedevelopers"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
